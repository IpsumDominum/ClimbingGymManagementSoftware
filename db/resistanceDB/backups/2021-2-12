--
-- Name: member; Type: TABLE; Schema: public; Owner: postgres
--

CREATE TABLE public.member (
    id character varying NOT NULL,
    "userType" character varying(50) NOT NULL,
    title character varying(50),
    "firstName" character varying(50) NOT NULL,
    "lastName" character varying(50) NOT NULL,
    "middleName" character varying(50) NOT NULL,
    address1 character varying(50),
    address2 character varying(50),
    "postalCode" character varying(50),
    city character varying(50),
    state character varying(50),
    country character varying(50),
    cellphone character varying(50),
    homephone character varying(50),
    workphone character varying(50),
    birthday character varying(50),
    email character varying(50),
    "emergencyContact" character varying(50),
    "emergencyContactPhone" character varying(50),
    "emergencyContactRelation" character varying(50),
    last_visit timestamp without time zone,
    last_edit timestamp without time zone,
    warning character varying,
    access_after_hours boolean,
    waiver_signed boolean,
    waiver_signed_date timestamp without time zone,
    "signatureImage" character varying,
    family character varying,
    status character varying(50) NOT NULL,
    proficiency_level character varying(50),
    got_to_know_channel character varying(50),
    mail_on_internal_events boolean,
    mail_promotions boolean,
    do_not_send_mail boolean,
    concession_passes integer,
    stripe_id character varying(50),
    stripe_invoice_prefix character varying(50),
    created timestamp without time zone
);


ALTER TABLE public.member OWNER TO postgres;

--
-- Name: membership; Type: TABLE; Schema: public; Owner: postgres
--

CREATE TABLE public.membership (
    id character varying NOT NULL,
    member_id character varying NOT NULL,
    created timestamp without time zone,
    start_date timestamp without time zone,
    end_date timestamp without time zone,
    paid integer NOT NULL,
    "membershipActive" boolean,
    expected_payment_amount integer NOT NULL,
    description character varying(50),
    next_billing_date timestamp without time zone,
    billing_frequency character varying(50),
    frozen boolean,
    frozen_until timestamp without time zone,
    on_holiday boolean,
    holiday_until timestamp without time zone,
    state character varying,
    "billingType" character varying(50)
);


ALTER TABLE public.membership OWNER TO postgres;

